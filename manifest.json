{
    "manifest_version": 2,
    "name": "iams",
    "description": "FB Emoji Chat Suggestions",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["fb.js"],
    "run_at": "document_end"
    }
    ],
    "permissions": [
    "tabs",
    "https://www.facebook.com/"
    ],
     "background": {
      "scripts": ["background.js"],
      "persistent": true
    }
}